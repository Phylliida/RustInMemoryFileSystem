{
  "name": "v86filesystemport",
  "version": "1.0.0",
  "description": "port v86 fs to assembly script to wasm",
  "main": "filesystem.js",
  "scripts": {
    "test": "node tests",
    "asbuild:debug": "asc assembly/index.ts --target debug",
    "asbuild:release": "asc assembly/index.ts --target release",
    "asbuild": "npm run asbuild:debug && npm run asbuild:release",
    "start": "npx serve ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Phylliida/v86FileSystemPort.git"
  },
  "author": "Phylliida",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Phylliida/v86FileSystemPort/issues"
  },
  "homepage": "https://github.com/Phylliida/v86FileSystemPort#readme",
  "devDependencies": {
    "assemblyscript": "^0.27.29"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./build/release.js",
      "types": "./build/release.d.ts"
    }
  }
}